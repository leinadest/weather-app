{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "rules": {
        "no-console": "off",
        "max-classes-per-file": ["error", 5],
        "no-param-reassign": ["error", { "props": false }]
    }
}
